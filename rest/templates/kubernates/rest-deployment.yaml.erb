apiVersion: apps/v1
kind: Deployment
metadata:
  name: rest-app-<%= @graph_id %>
  namespace: <%= @organizationID %>
  labels:
    app: rest-app-<%= @graph_id %>
spec:
  replicas: 1
  selector:
    matchLabels:
      app: rest-app-<%= @graph_id %>
  template:
    metadata:
      labels:
        app: rest-app-<%= @graph_id %>
    spec:
      containers:
      - name: rest<%= @graph_id %>
        image: ravi8881/<%= @organizationID %>-<%= @sub_organization_id %>-<%= @enviroment_id %>-<%= @graph_id %>-rest:<%= @version %>
        ports:
        - containerPort: 8081
